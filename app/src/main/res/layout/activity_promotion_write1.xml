<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:visibility="visible"
    tools:context=".PromotionWrite1">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:lineSpacingExtra="8sp"
        android:text="모집 기간"
        android:textColor="#707070"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText5"
        app:layout_constraintStart_toEndOf="@+id/view8" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:lineSpacingExtra="8sp"
        android:text="회비"
        android:textColor="#707070"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText4"
        app:layout_constraintStart_toEndOf="@+id/view7" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:lineSpacingExtra="8sp"
        android:text="면접"
        android:textColor="#707070"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText3"
        app:layout_constraintStart_toEndOf="@+id/view6" />

    <View
        android:id="@+id/view8"
        android:layout_width="3dp"
        android:layout_height="25dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/btn_round_blue_box"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText4"></View>

    <View
        android:id="@+id/view7"
        android:layout_width="3dp"
        android:layout_height="25dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/btn_round_blue_box"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText3"></View>

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:background="#00000000"
        android:src="@drawable/arrow_left"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></ImageView>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="152dp"
        android:layout_height="58dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="22dp"
        android:fontFamily="@font/notosanskrbold"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="12sp"
        android:text="홍보글 작성"
        android:textColor="#3c3c3c"
        android:textSize="28sp"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent" />

    <android.widget.Button
        android:id="@+id/button_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:layout_marginTop="39dp"
        android:layout_marginEnd="30dp"
        android:background="@drawable/btn_round_blue_box"
        android:text="다음"
        android:textColor="#ffffff"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view2"
        android:layout_width="3dp"
        android:layout_height="25dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/btn_round_blue_box"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"></View>

    <View
        android:id="@+id/view"
        android:layout_width="3dp"
        android:layout_height="25dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/btn_round_blue_box"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText"></View>

    <View
        android:id="@+id/view6"
        android:layout_width="3dp"
        android:layout_height="25dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="14dp"
        android:background="@drawable/btn_round_blue_box"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextText2"></View>

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="355dp"
        android:layout_height="48dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="27dp"
        android:ems="10"
        android:inputType="text"
        android:text="게시글의 제목을 입력해 주세요."
        android:textColor="#29000000"
        android:background="@drawable/edittext_rounded_corner_rectangle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="355dp"
        android:layout_height="48dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="27dp"
        android:background="@drawable/edittext_rounded_corner_rectangle"
        android:ems="10"
        android:inputType="text"
        android:text="모임 이름을 입력해 주세요."
        android:textColor="#29000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="355dp"
        android:layout_height="48dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="27dp"
        android:background="@drawable/edittext_rounded_corner_rectangle"
        android:ems="10"
        android:inputType="text"
        android:text="면접 유무를 입력해 주세요."
        android:textColor="#29000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view6" />

    <EditText
        android:id="@+id/editTextText4"
        android:layout_width="355dp"
        android:layout_height="48dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="27dp"
        android:background="@drawable/edittext_rounded_corner_rectangle"
        android:ems="10"
        android:inputType="text"
        android:text="회비를 입력해 주세요."
        android:textColor="#29000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view7" />

    <EditText
        android:id="@+id/editTextText5"
        android:layout_width="355dp"
        android:layout_height="48dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="27dp"
        android:background="@drawable/edittext_rounded_corner_rectangle"
        android:ems="10"
        android:inputType="date"
        android:text="모집 기간을 입력해 주세요."
        android:textColor="#29000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view8" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:lineSpacingExtra="8sp"
        android:text="글제목"
        android:textColor="#707070"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText"
        app:layout_constraintStart_toEndOf="@+id/view2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="16dp"
        android:lineSpacingExtra="8sp"
        android:text="모임 이름"
        android:textColor="#707070"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/editTextText2"
        app:layout_constraintStart_toEndOf="@+id/view" />

</androidx.constraintlayout.widget.ConstraintLayout>